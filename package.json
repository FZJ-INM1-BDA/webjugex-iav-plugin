{
  "name": "web-jugex",
  "version": "1.0.0",
  "description": "WebJuGEx is a simple and intuitive graphical user interface for [JuGEx](https://pyjugex.readthedocs.io/) - a workflow for analyzing differential gene expressions in different regions of the JuBrain probabilistic cytoarchitectonic human brain atlas.",
  "main": "index.js",
  "scripts": {
    "test": "mocha-webpack --webpack-config webpack.config.js --require test/setup.js",
    "karma": "karma start",
    "dev": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "build-ssr-client": "webpack --config ./vueSsr/webpack.client.config.js",
    "build-ssr-server": "webpack --config ./vueSsr/webpack.server.config.js",
    "build-ssr": "npm run build-ssr-client && npm run build-ssr-server"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
