    "@babel/register": "^7.9.0",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "dotenv": "^7.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.2.3",
    "mocha-webpack": "^2.0.0-beta.0",
